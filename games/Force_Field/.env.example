# Force Field Game Configuration
# Copy this file to .env and customize as needed

# Game Settings
FORCE_FIELD_DEBUG=false
FORCE_FIELD_LOG_LEVEL=INFO
FORCE_FIELD_FULLSCREEN=false

# Performance Settings
FORCE_FIELD_RENDER_SCALE=4
FORCE_FIELD_TARGET_FPS=60

# Audio Settings
FORCE_FIELD_MASTER_VOLUME=0.7
FORCE_FIELD_MUSIC_VOLUME=0.5
FORCE_FIELD_SFX_VOLUME=0.8

# Development Settings
FORCE_FIELD_ENABLE_CHEATS=false
FORCE_FIELD_SHOW_DEBUG_INFO=false